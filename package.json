	},
	"devDependencies": {
		"@11ty/eleventy": "^1.0.1",
		"@prettier/plugin-pug": "^2.0.0",
		"@wordpress/browserslist-config": "^4.1.2",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.1.0",
		"cross-env": "^7.0.3",
		"drygen": "^0.0.5",
		"postcss-hover-media-feature": "^1.0.2",
		"postcss-import": "^14.1.0",
		"prettier": "^2.6.2",
		"prettier-plugin-css-order": "^1.2.0",
		"rimraf": "^3.0.2",
		"rollup-plugin-visualizer": "^5.6.0",
